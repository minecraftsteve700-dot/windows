{
  "name": "Windows 11 Pro",
  "service": "windows",
  "containerEnv": {
    "VERSION": "11"
  },
  "hostRequirements": {
   "cpus": 8,
   "memory": "32gb",
   "storage": "64gb"
  },
  "forwardPorts": [8006],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "dockerComposeFile": "codespaces.yml",
  "initializeCommand": "docker system prune --all --force"
}
